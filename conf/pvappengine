server {
    listen 8000;
    server_name localhost;

    location = /favicon.ico { access_log off; log_not_found off; }
    
	location /static/ {
        root /usr/share/flh-pvappengine;
    }

    location / {
        include uwsgi_params;
        uwsgi_pass unix:/var/run/flh-pvappengine.sock;
    }
}

